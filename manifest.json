{
  "manifest_version": 2,
  "name": "DiBox",
  "version": "1.0.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/js/vue.min.js", "src/js/jquery-1.8.3.js", "dist/demo.js"]
    }
  ],
  "background": {
    "page": "background.html",
    "type": "module"
  },
  "permissions":[ "contextMenus", "selection", "tabs"]

}